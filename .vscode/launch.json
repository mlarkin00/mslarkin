{
  "configurations": [
    
    {
      "name": "loadgenConfigLocal",
      "type": "cloudcode.cloudrun",
      "request": "launch",
      "build": {
        "docker": {
          "path": "loadgen-utils/loadgenConfig/Dockerfile"
        }
      },
      "image": "loadgen-config",
      "service": {
        "name": "loadgen-config",
        "containerPort": 8080,
        "resources": {
          "limits": {
            "memory": "256Mi"
          }
        }
      },
      "target": {
        "minikube": {}
      },
      "watch": true
    },
    {
      "name": "requestloadgenLocal",
      "type": "cloudcode.cloudrun",
      "request": "launch",
      "build": {
        "docker": {
          "path": "loadgen-utils/requestLoadgen/Dockerfile"
        }
      },
      "image": "request-loadgen",
      "service": {
        "name": "request-loadgen",
        "containerPort": 8080,
        "resources": {
          "limits": {
            "memory": "256Mi"
          }
        }
      },
      "target": {
        "minikube": {}
      },
      "watch": true
    }
  ]
}
